package ontology;

import jade.content.Predicate;
import jade.core.AID;
import jade.util.leap.List;

public class Costs implements Predicate
	private Book item;
	private int price;
	
	public Book getItem() {
		return item;
	}
	public void setItem(Book item) {
		this.item = item;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
}
